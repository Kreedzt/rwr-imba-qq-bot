    "dotenv": "^16.0.3",
    "echarts": "^5.4.3",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.0.11",
    "lodash": "^4.17.21",
    "pinyin-match": "^1.2.4",
    "tracer": "^1.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
